package Operators_in_java;

public class Unary_Operator {

	public static void main(String[] args) {
		
     int a=10;
     System.out.println(a++);//a+1 
     System.out.println(++a);//1+a 
     System.out.println(a--);//a-1 
     System.out.println(--a);//1-a 
     
     
     
     
	}

}
                             