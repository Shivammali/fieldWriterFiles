package Spacial_Logic_Programme;

public class Even_Odd_numbers {

	public static void main(String[] args) {

		for( int i=2;i<=10;i++) {
			
		
		if(i%2==0) 
		{
		System.out.println(i+" even number");
		
		}
		for( int j=2;j<=10;j++) {	
			
			if(j%2!=0) 
			{
			System.out.println(j+" odd number");
		
			}	
			
		
		
		
		
		//else {
			//System.out.println(i+" odd number");	
			
		}	
		}
	
	}
	}
