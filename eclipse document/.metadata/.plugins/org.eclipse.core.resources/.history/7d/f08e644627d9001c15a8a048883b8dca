package Operators_in_java;

public class Unary_Operator {

	public static void main(String[] args) {
		
     int a=10;
     System.out.println(++a);//1+a 26
     System.out.println(a++);//a+1 27
     System.out.println(a--);//a-1 26
     System.out.println(--a);//1-a 25
     
     
     
     
	}

}
