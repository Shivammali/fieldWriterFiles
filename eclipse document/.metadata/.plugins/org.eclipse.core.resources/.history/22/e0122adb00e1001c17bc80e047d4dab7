package Spacial_Logic_Programme;

public class prime_numbers {
	
	public static void main(String[]args) {
		
		int a=17;
		int b=0;
		
		for(int i=2;i<=a-1;i++) {
			
			if(a%i==0) {
				//b++;
				//break;
			}
			}
			if(b==0) {
				
			System.out.println("given no is prime");	
				}	
			else{
				
				System.out.println("given no is not prime");	
			}	
				
	}

}
