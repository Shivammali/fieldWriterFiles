package controlstatement;

public class if_statement {

	public static void main(String[] args) {
		
		int salary=15000;
		
		if(salary>15000) {
			System.out.println("your salary is greater than 15000:");		
		}
	  {
			
			System.out.println("your salary is not greater than 15000:");
			
		
	
	}

}
}